<Window x:Class="QuizTool.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuizTool.UI" 
        mc:Ignorable="d"
        Topmost="False"
        WindowStyle="None"
        WindowState="Normal"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        
        
        Icon="logo.png"
        Title=" Question" MinHeight="480" MinWidth="800" Loaded="Window_Loaded">
    <Grid Loaded="Grid_Loaded">
            <Frame x:Name="mainFrame" BorderThickness="0" NavigationUIVisibility="Hidden" Background="#FF003459"/>
    </Grid>
</Window>
